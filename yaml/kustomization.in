apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization

namespace: @namespace@

nameSuffix: -@owner@

secretGenerator:
- name: actions-runner
  literals:
  - GITHUB_TOKEN=@github_token@

patchesStrategicMerge:
- github-settings.yaml

resources:
  - deployment.yaml
  - hpa.yaml
  - github-packages-secret.yaml

